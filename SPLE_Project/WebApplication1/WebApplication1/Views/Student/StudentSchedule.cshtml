@model WebApplication1.Models.StudentModel

<body>
    <br />
    <br />
    <div class="container">
        <div class="col-md-9">
            <h2>Schedule</h2>
        </div>
        <div class="col-md-3">
            <button class="btn btn-success">Add new item</button>
        </div>
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <td>Date</td>
                    <td>Note</td>
                </tr>
            </thead>
            <tbody>
                <tr>
                    @if (Model.stu.Schedule != null)
                    {
                        foreach (var elem in Model.stu.Schedule)
                        {
                            <td>@elem.Date</td>
                            <td>@elem.DateNote</td>
                        }

                    }
                </tr>
            </tbody>
        </table>
        <br />
        <br />
    </div>
</body>
