<div class="container">
    <div class="col-lg-8">
        <h2>Schedule</h2>
        <p>This is the schedule for user @ViewBag.User.FullName</p>
        <div class="clearfix"></div>
    </div>
    <div class="clearfix"></div>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <td>Date</td>
                <td>Note</td>
                <td>Operations</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var element in ViewBag.ScheduleList)
            {
                <tr>
                    <td>@element.Date</td>
                    <td>@element.DateNote</td>
                    <td><button class="btn btn-xs">Edit</button><button class="btn btn-xs btn-danger">Delete</button>
                    <td>

                </tr>
            }
        </tbody>

    </table>
</div>
<br />
<br />
<div class="container">
    <form>
        <div class="form-group">
            <label for="DateNote">Note</label>
            <textarea rows="4" class="col-md-8" name="DateNote"></textarea>
        </div>
        <div class="form-group">
            <label for="Date">Date</label>
            <input type="datetime" class="col-md-8" name="Date">
        </div>
    </form>
    <button class="btn btn-success">Add</button>
</div>