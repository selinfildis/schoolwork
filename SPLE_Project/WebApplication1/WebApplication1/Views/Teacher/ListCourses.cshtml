@model WebApplication1.Models.TeacherModel
<body>
    <div class="container">
        <h2>List of Courses Given</h2>
        <button class="btn btn-success">Add Course</button>
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <td>Course Name</td>
                    <td>Course Code</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var element in Model.teach.Course)
                {
                    <tr>
                        <td>@element.CourseName</td>
                        <td>@element.cid</td>
                    </tr>
                }
            </tbody>

        </table>
    </div>
</body>
